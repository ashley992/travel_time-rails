<div class='center grey-box'>
  <h1>Edit Profile</h1><br>
  <%= form_for(@agency) do |f| %>
    <p><%= f.label :name %>
    <%= f.text_field :name%></p><br>

    <p><%= f.label :address %>
    <%= f.text_field :address %></p><br>

    <p><%= f.label :phone_number %>
    <%= f.phone_field :phone_number %></p><br>

    <p><%= f.label :country %>
    <%= f.text_field :country %></p><br>

    <p><%= f.label :language %>
    <%= f.text_field :language %></p><br> 
    
    <p><%= f.submit %>
  <% end %>
</div>